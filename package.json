    "fire:beta": "firebase hosting:channel:deploy test"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.18",
    "@sveltejs/kit": "next",
    "@tsconfig/svelte": "^2.0.1",
    "firebase": "^9.8.1",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.56.3",
    "svelte": "^3.34.0",
    "svelte-check": "^2.2.5",
    "svelte-preprocess": "^4.9.0",
    "svelte-windicss-preprocess": "^4.0.12",
    "three": "^0.132.2",
    "tslib": "^2.0.0",
    "typescript": "^4.4.2"
  },
  "type": "module"
}
